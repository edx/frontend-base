{
  "name": "@edx/frontend-base",
  "version": "1.0.0-semantically-released",
  "description": "Foundational bootstrapping code for Open edX micro-frontend applications.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "make build",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-base.git"
  },
  "files": [
    "/dist"
  ],
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-base/issues"
  },
  "homepage": "https://github.com/edx/frontend-base#readme",
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "8.1.0",
    "@commitlint/prompt": "8.1.0",
    "@commitlint/prompt-cli": "8.1.0",
    "@edx/frontend-analytics": "3.0.0",
    "@edx/frontend-auth": "7.0.1",
    "@edx/frontend-build": "1.3.1",
    "@edx/frontend-i18n": "3.0.3",
    "@edx/frontend-logging": "3.0.1",
    "@edx/paragon": "7.1.5",
    "babel-polyfill": "6.26.0",
    "codecov": "3.5.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "husky": "3.0.9",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-saga": "1.1.2"
  },
  "dependencies": {
    "history": "4.9.0",
    "lodash.camelcase": "4.3.0",
    "lodash.memoize": "4.1.2",
    "lodash.merge": "4.6.2",
    "lodash.snakecase": "4.1.1",
    "pubsub-js": "1.7.0"
  },
  "peerDependencies": {
    "@edx/frontend-analytics": "^3.0.0",
